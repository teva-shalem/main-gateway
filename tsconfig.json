{
    "compilerOptions": {
        "target": "esnext",
        "module": "es2015",
        "declaration": false,
        "declarationMap": false,
        "sourceMap": true,
        "downlevelIteration": true,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "moduleResolution": "node",
        "baseUrl": "./",
        "esModuleInterop": true
    },
    "include": [
        "app"
    ]
}
